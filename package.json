{
  "name": "newshub",
  "displayName": "Newshub",
  "version": "0.0.1",
  "description": "Save pages to a NewsHub instance while browsing.",
  "author": "Mathieu Deous",
  "private": true,
  "scripts": {
    "dev:chrome": "plasmo --target=chrome-mv3 dev",
    "dev:firefox": "plasmo --target=firefox-mv2 dev",
    "build": "plasmo --target=chrome-mv3 build && plasmo --target=firefox-mv2 build",
    "build:chrome": "plasmo --target=chrome-mv3 build",
    "build:firefox": "plasmo --target=firefox-mv2 build",
    "package": "plasmo --target=chrome-mv3 package && plasmo --target=firefox-mv2 package",
    "package:chrome": "plasmo --target=chrome-mv3 package",
    "package:firefox": "plasmo --target=firefox-mv2 package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/parcel-config": "^0.41.0",
    "@plasmohq/storage": "^1.13.0",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "plasmo": "0.89.4",
    "react": "18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-bootstrap-icons": "^1.11.4",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.4.0",
    "@types/chrome": "0.0.287",
    "@types/node": "22.10.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "prettier": "3.3.3",
    "typescript": "5.7.2"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "storage"
    ]
  }
}
